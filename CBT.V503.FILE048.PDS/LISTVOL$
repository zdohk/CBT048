//SBGOLOBU JOB (CCMVS),ASMLINK,
// NOTIFY=&SYSUID,
// CLASS=A,MSGCLASS=X,COND=(1,LT)
//*
//*     ASM LINKEDIT FROM SOURCE ASM
//*
//ASMH EXEC PGM=ASMA90,PARM=(OBJECT,NODECK,NOESD,NORLD,NOXREF,FLAG(5))
//SYSLIB   DD DISP=SHR,DSN=SBGOLOB.R16.MACLIB
//         DD DISP=SHR,DSN=SBGOLOB.R16.MODGEN
//SYSIN    DD DISP=SHR,DSN=SBGOLOB.B.ASM(LISTVOL)
//         DD DISP=SHR,DSN=SBGOLOB.B.ASM(LOCINDEX)
//SYSUT1   DD UNIT=VIO,SPACE=(CYL,2)
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD UNIT=VIO,SPACE=(TRK,1),DISP=(,PASS),DCB=BLKSIZE=3200,
//            DSN=&&TEMP
//*
//LKED   EXEC  PGM=HEWL,
//       PARM='NCAL,MAP,LIST,LET'
//SYSUT1   DD  UNIT=SYSDA,SPACE=(2048,(200,20))
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=SBGOLOB.W$$.LINKLIB
//SYSLMOD  DD  DISP=SHR,DSN=SBGOLOB.W$$.LINKLIB
//OBJECT   DD  DISP=(OLD,DELETE),DSN=&&TEMP
//SYSLIN   DD  *
 INCLUDE OBJECT
 SETSSI  CB470247
 SETCODE AC(0)
 ENTRY   LISTVOL
 ALIAS   LISTV
 ALIAS   LV
 NAME    LISTVOL(R)
